      "source": [
        "def count_up(n):\n",
        "    count = 1\n",
        "    while count <= n:\n",
        "        yield count\n",
        "        count += 1\n",
        "\n",
        "generator = count_up(3)\n",
        "print(next(generator))\n",
        "print(next(generator))\n",
        "print(next(generator))"
      ]
